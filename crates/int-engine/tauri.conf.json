{
    "productName": "INT Installer",
    "version": "0.2.0",
    "identifier": "com.int.installer",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:5173",
        "frontendDist": "src-ui/dist"
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "INT Package Installer",
                "width": 500,
                "height": 380,
                "resizable": false,
                "center": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.png"
        ]
    },
    "plugins": {
        "shell": {
            "open": true
        }
    }
}